ackground:white;\n    position: absolute;\n    transition:all .15s cubic-bezier(0.71, 0.03, 0.56, 0.85);\n    bottom:0px;\n    left:0px;\n}\n.Share_circleOpen{\n    background:none;\n}\n.Share_circleHalf{\n    height:30px;\n    width:60px;\n    border-bottom-left-radius: 30px;\n    border-bottom-right-radius: 30px;\n    border-top: 0;\n    box-shadow: 0 -5px 5px inset rgb(240,240,240);\n    transition:all .15s cubic-bezier(0.71, 0.03, 0.56, 0.85);\n    position: absolute;\n    bottom:0px;\n    left:0px;\n}\n.Share_circleHalfOpen{\n    height:0px;\n    bottom:30px;\n}\n.Share_button{\n    height:44px;\n    width:44px;\n    border-radius:50% ;\n    cursor: pointer;\n    box-shadow: 0px 3px 5px -1px rgba(0,0,0,0.2), 0px 6px 10px 0px rgba(0,0,0,0.14), 0px 1px 18px 0px rgba(0,0,0,0.12);\n    background: linear-gradient(120deg, #7093FF 0%,  #3D5AFE 90%);\n    text-align: center;\n    position: absolute;\n    transition:all .15s cubic-bezier(0.71, 0.03, 0.56, 0.85);\n    top:8px;\n    left:8px;\n    z-index: 2;\n}\n.Share_button:hover{\n    box-shadow: 0px 3px 5px -1px rgba(0,0,0,0.25), 0px 6px 10px 0px rgba(0,0,0,0.2), 0px 1px 18px 0px rgba(0,0,0,0.17);\n}\n.Share_buttonDisabled{\n    background: linear-gradient(120deg, rgb(210,210,210) 0%,  rgb(170,170,170) 90%);\n}\n.Share_buttonDisabled:hover{\n    cursor: default;\n    box-shadow: 0px 3px 5px -1px rgba(0,0,0,0.2), 0px 6px 10px 0px rgba(0,0,0,0.14), 0px 1px 18px 0px rgba(0,0,0,0.12);\n}\n.Share_icon{\n    color:white;\n    font-size:18px;\n    width:18px;\n    margin-right:calc(50% - 9px);\n    margin-top:calc(50% - 9px);\n    transition:all .25s cubic-bezier(0.71, 0.03, 0.56, 0.85);\n    pointer-events: none;\n    position: absolute;\n    right:0px;\n    top:0px;\n    z-index: 3;\n}\n.Share_iconOpen{\n    margin-top:32px;\n    margin-right:46px;\n    color:var($color);\n    pointer-events: auto;\n}\n.Share_iconActive{\n    cursor: pointer;\n    color:$main;\n}\n.Share_iconSendAffect{\n    animation: flyaway 1.3s linear;\n}\n@keyframes flyaway {\n    0%   { transform: rotate(10deg);\n           top: 13px;\n           right: 25px;\n           height: 30px; }\n    5%   { transform: rotate(10deg);\n           top: 13px;\n           right: 0px;\n           height: 30px; }\n    20%   { transform: rotate(-20deg);\n            top: 13px;\n            right: -130px;\n            height: 45px; }  \n    40%   { transform: rotate(10deg);\n            top: -40px;\n            right: -280px;\n            opacity: 1; }\n    100% { transform: rotate(60deg);\n           top: -200px;\n           right: -1000px;\n           height: 0;\n           opacity: 0; }\n  }\n.Share_buttonOpen{\n    width:312px;\n    height:286px;\n    cursor: default;\n    font-family:$font;\n    color:var($color);\n    text-align: left;\n    font-size:14px;\n    border-radius: 4px;\n    background:white;\n    box-shadow: 0px 6px 10px 0px rgba(0,0,0,0.07), 0px 1px 18px 0px rgba(0,0,0,0.06);\n    transform: translate(-50%,-50%);\n    left:50%;\n    top:50%;\n}\n.Share_buttonOpen:hover{\n    box-shadow:  0px 6px 10px 0px rgba(0,0,0,0.07), 0px 1px 18px 0px rgba(0,0,0,0.06);\n}\n.Share_title{\n    margin-top:20px;\n}\n.Share_box{\n    height:calc(100% - 2px);\n    width:calc(100% - 2px);\n    background:white;\n    border-radius: 50%;\n    overflow: hidden;\n    opacity: 0;\n    transition:all .15s cubic-bezier(0.71, 0.03, 0.56, 0.85);\n    box-sizing: border-box;\n    padding:0px 20px;\n    position: absolute;\n    top:1px;\n    left:1px;\n}\n.Share_or{\n    width:100%;\n    text-align: center;\n    margin:20px 0px;\n    position: relative;\n}\n.Share_orText{\n    padding:0px 20px;\n    background:white;\n    font-weight: 400;\n    position: relative;\n    z-index: 2;\n}\n.Share_line{\n    width:100%;\n    height:1px;\n    background:rgb(220,220,220);\n    z-index: 1;\n    position: absolute;\n    top:50%;\n}\n.Share_linkBox{\n    display: flex;\n    margin-top:16px;\n    align-items: center;\n}\n.Share_link{\n    padding:5px;\n    background:rgb(250,250,250);\n    border:1px rgb(208, 214, 224) solid;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    white-space: nowrap;\n    color:var($color);\n    border-radius: 4px;\n    text-decoration:underline;\n    transition:0.2s all;\n}\n.Share_link a {\n    color:var($color);\n}\n.Share_link a:hover{\n    color:$main;\n}\n.Share_copy{\n    margin-left:6px;\n    cursor:pointer;\n    transition:0.2s all;\n}\n.Share_copy:hover{\n    color:$main;\n}\n.Share_boxOpen{\n    border-radius: 4px;\n    opacity:1\n}\n.Share_close{\n    width:8px;\n    cursor: pointer;\n    color:var($color);\n    transition:all .15s cubic-bezier(0.71, 0.03, 0.56, 0.85);\n    position: absolute;\n    top:8px;\n    right:8px;\n}\n.Share_close:hover{\n    color:black;\n}\n.Share_input{\n    width:calc(100% - 30px);\n    margin-top:20px;\n    position: relative;\n}\n.share_progress{\n    width:100%;\n    margin:20px 0px;\n    float:left;\n}\n.share_progress_number{\n    font-size:18px;\n    font-weight: bold;\n    margin-left:4px;\n}"],"sourceRoot":""}]);
