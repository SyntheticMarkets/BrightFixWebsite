48, 66) solid;\n$color: rgb(255, 255, 255);\n$color-soft: rgb(190, 190, 190);\n\n$input-bg: rgb(45, 45, 65);\n$input-bg-hover: rgb(65, 65, 95);\n$input-color: rgb(162, 163, 190);\n$input-color-bold: rgb(162, 163, 200);\n$input-placeholder: rgb(82, 83, 100);\n\n$label-width: 90px;\n$modal-shadow: 0px 0px 4px rgb(20, 20, 20);\n\n$blur: blur(15px);\n// Import the variables from the JavaScript file\n\n","@import \"../../colors.scss\";\n.TextAI {\n  width: 96%;\n  border-radius: 38px;\n  transform: translate(0, 50%);\n  background: var(--xx-bg-blur);\n  backdrop-filter: $blur;\n  box-shadow: var(--xx-bg-shadow);\n  box-sizing: border-box !important;\n  padding: 4px;\n  display: flex;\n  position: absolute;\n  bottom: 0px;\n  left: 2%;\n}\n.TextAI_onButton {\n  border-radius: 8px;\n  transform: translate(0, 0);\n  transition: 0.4s all;\n  border: 1px rgb(65, 71, 80) solid;\n\n  .textarea {\n    height: 36px;\n    outline: none;\n    border: none;\n    border-radius: 8px;\n    background: var(--xx-bg-blur);\n    box-shadow: none;\n\n    &::placeholder {\n      color: rgba(255, 255, 255, 0.4);\n      transition: 0.2s all;\n    }\n\n    &:focus,\n    &:hover,\n    &:active {\n      background: var(--xx-bg-blur);\n    }\n  }\n}\n.TextAI_focus {\n  border: 1px white solid !important;\n}\n.textarea {\n  flex-grow: 1;\n  height: 40px;\n  background: rgba(0, 0, 0, 0.3);\n  border-radius: 28px;\n  box-shadow: inset 1.46px 2.92px 2.92px -0.73px rgba(0, 0, 0, 0.25),\n    inset 0px -0.73px 0.73px 0px rgba(0, 0, 0, 0.35);\n  border: none;\n  color: rgba(255, 255, 255, 0.6);\n  font-size: 12px;\n  font-family: $font;\n  transition: 0.2s all;\n  padding: 4px 16px;\n  resize: none;\n  box-sizing: border-box !important;\n  transition: 0.2s all;\n  outline: none;\n\n  &::placeholder {\n    color: rgba(255, 255, 255, 0.8);\n    transition: 0.2s all;\n  }\n  &:focus,\n  &:hover,\n  &:active {\n    background: rgba(0, 0, 0, 0.5);\n    border-color: $main;\n    color: white;\n  }\n}\n.error {\n  width: 230px;\n  background: $bg-hard;\n  transform: translateX(-50%);\n  padding: 8px 10px;\n  border-radius: 4px;\n  color: $color;\n  font-family: $font;\n  text-align: center !important;\n  position: absolute;\n  top: -36px;\n  left: 50%;\n\n  &:before {\n    position: absolute;\n    content: \"\";\n    height: 8px;\n    width: 8px;\n    background: $bg-hard;\n    bottom: -3px;\n    left: 50%;\n    transform: translate(-50%) rotate(45deg);\n    transition: all 0.3s cubic-bezier(0.68, -0.55, 0.265, 1.55);\n    z-index: -1;\n  }\n}\n"],"sourceRoot":""}]);
