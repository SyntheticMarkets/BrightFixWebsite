n  }\n}\n.row {\n  margin-top: 8px;\n}\n.have_streaming {\n  width: 100%;\n  box-sizing: border-box !important;\n}\n.default_message {\n  width: 100%;\n  color: white;\n  max-width: 100%;\n  background: none;\n  padding: 6px 0px;\n\n  span {\n    text-align: center !important;\n    display: flex;\n  }\n}\n.codeChanges {\n  cursor: pointer;\n}\n.user_message {\n  margin-left: auto;\n  color: white;\n  background: rgba(38, 38, 40);\n  right: 4px;\n  left: initial;\n}\n.small_icon {\n  position: absolute;\n  bottom: -4px;\n  left: -4px;\n}\n.small_iconRight {\n  left: initial;\n  right: -4px;\n}\n.button {\n  width: 80%;\n  font-size: 12px;\n  border-radius: 4px;\n  color: white;\n  margin-top: 10px;\n  text-align: center !important;\n  background: $main;\n  padding: 6px 0px;\n  cursor: pointer;\n  font-weight: 600;\n  transition: 0.2s all;\n  box-shadow: 0 0 8px rgba(0, 0, 0, 0.2);\n  position: relative;\n  left: 10%;\n}\n.buttonAffect:after {\n  position: absolute;\n  content: \"\";\n  width: 0;\n  height: 100%;\n  top: 0;\n  left: 0;\n  direction: rtl;\n  z-index: -1;\n  box-shadow: -7px -7px 20px 0px #fff9, -4px -4px 5px 0px #fff9,\n    7px 7px 20px 0px #0002, 4px 4px 5px 0px #0001;\n  transition: all 0.3s ease;\n}\n.buttonAffect:hover {\n  transform: scale(1.05);\n}\n.buttonAffect:active {\n  top: 2px;\n}\n\n.buttonDisabled {\n  cursor: default;\n  background: rgb(200, 200, 200);\n  opacity: 0.9;\n}\n.loadWrapp {\n  float: left;\n  width: 100px;\n  height: 100px;\n  margin: 0 10px 10px 0;\n  padding: 20px 20px 20px;\n  border-radius: 5px;\n  text-align: center;\n  background-color: #d8d8d8;\n}\n\n.loadingWrapper {\n  height: 40px;\n  width: 100%;\n  padding-top: 10px;\n  padding-bottom: 4px;\n  display: flex;\n  justify-content: center;\n  position: relative;\n}\n.loadingWrapper_percentage {\n  color: white;\n  transform: translate(-50%, 0%);\n  line-height: 40px;\n  position: absolute;\n  left: 50%;\n}\n\n.dot {\n  display: inline-block;\n  width: 6px;\n  height: 6px;\n  border-radius: 50%;\n  margin-left: 4px;\n  background-color: $main;\n}\n\n.loading .dot:nth-last-child(1) {\n  animation: loadingC 1s 0.1s linear infinite;\n}\n.loading .dot:nth-last-child(2) {\n  animation: loadingC 1s 0.2s linear infinite;\n}\n.loading .dot:nth-last-child(3) {\n  animation: loadingC 1s 0.3s linear infinite;\n}\n\n@keyframes loadingC {\n  0% {\n    transform: translate(0, 0);\n  }\n  50% {\n    transform: translate(0, 15px);\n  }\n  100% {\n    transform: translate(0, 0);\n  }\n}\n"],"sourceRoot":""}]);
