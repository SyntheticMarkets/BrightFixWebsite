n$color-soft: rgb(190, 190, 190);\n\n$input-bg: rgb(45, 45, 65);\n$input-bg-hover: rgb(65, 65, 95);\n$input-color: rgb(162, 163, 190);\n$input-color-bold: rgb(162, 163, 200);\n$input-placeholder: rgb(82, 83, 100);\n\n$label-width: 90px;\n$modal-shadow: 0px 0px 4px rgb(20, 20, 20);\n\n$blur: blur(15px);\n// Import the variables from the JavaScript file\n\n","\n@import '../colors.scss';\n.Input_input{\n    width:calc(100% - 0px);\n    padding:10px;\n    border-radius: 4px;\n    outline: none;\n    border:1px rgb(200,200,200) solid;\n    background:white;\n    color:black;\n    font-size:14px;\n    transition: all 0.25s cubic-bezier(0.71, 0.03, 0.56, 0.85);\n    box-sizing: border-box !important;\n    margin-top:10px;\n    position: relative;\n    float:left;\n}\n.inputDisable{\n    opacity:0.5;\n}\n.Input_input:hover{\n    border:1px $main solid;\n}\n.Input_input:focus{\n    border:1px $main solid;\n    box-shadow: 0px 10px 12px -8px rgba(32, 56, 117, 0.45);\n}\n.Input_title{\n    color:var($color);\n    font-size:14px;\n    font-family: $font;\n    text-overflow: ellipsis;\n    overflow: hidden; \n    font-weight: 400;\n    text-align: left;\n    position: relative;\n}\n.Input_invalid{\n    font-family: $font;\n    font-size:12px;\n    color:red;\n    position: relative;\n    float:left;\n    top:4px;\n}\n.Input_invalidInput{\n    color:red;\n}\n.Input_leftLabel{\n    display: flex;\n    transition:0.2s all;\n    flex-wrap: wrap;\n    align-items: center;\n    position: relative;\n    font-size:12px;\n}\n.Input_topLabel{\n    flex-direction: column;   \n    align-items: flex-start;\n    \n    .Input_input {\n        width: 100% !important;\n    }\n}\n.Input_leftLabel .Input_title {\n    width: $label-width;\n    font-size:12px;\n    color:$color;\n}\n.Input_leftLabel .Input_input{\n    width:calc(100% - #{$label-width});\n    background:$input-bg;\n    border:none;\n    font-size:12px;\n    color:$input-color;\n    padding:6px 8px;\n    margin-top:0px;\n}\n.Input_leftLabel .Input_input:hover, .Input_leftLabel .Input_input:focus {\n    background:$input-bg-hover;\n    color:white;\n}\n.Input_leftLabel_inputCurser input {\n    cursor: pointer;\n}\n.Input_search{\n    position: relative;\n    color:$input-color;\n}\n.Input_search .Input_input{\n    padding-left:30px;\n}\n.Input_input::placeholder{\n    color:$input-placeholder;\n}\n.Input_search_icon {\n    color:$input-color;\n    transition: all 0.25s cubic-bezier(0.71, 0.03, 0.56, 0.85);\n    position: absolute;\n    left:6px;\n}\n.Input_search:hover .Input_search_icon {\n    color:white;\n}"],"sourceRoot":""}]);
