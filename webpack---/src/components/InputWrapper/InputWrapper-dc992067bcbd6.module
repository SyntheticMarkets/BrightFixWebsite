idth: 31px;\n    line-height: 15px;\n    font-size:12px;\n    padding:10px 0px;\n    text-align: center;\n    z-index: 1;\n}\n.InputWrapper_selectList .MuiSelect-iconOutlined{\n    display: none;\n}\n.InputWrapper_selectList{\n    border-radius: 0px 4px 4px 0px;\n}\n.InputWrapper_list{\n    font-size:14px !important;\n}\n.InputWrapper_leftLabel {\n    width:100%;\n    align-items: center;\n    display: flex;\n}\n.InputWrapper_leftLabel .InputWrapper_input {\n    background:$input-bg;\n    border-radius: 4px;\n    border:none;\n    transition:0.2s all;\n}\n.InputWrapper_leftLabel .InputWrapper_input:hover, .InputWrapper_leftLabel .Mui-focused {\n    background:$input-bg-hover !important;\n}\n.InputWrapper_leftLabel .InputWrapper_input{\n    width: calc(100% - #{$label-width});\n    position: relative;\n}\n.InputWrapper_leftLabel .InputWrapper_input input {\n    width: 100%;\n}\n.InputWrapper_leftLabel .InputWrapper_input input{\n    height:8px;\n    color:$input-color !important;\n    transition:0.2s all;\n}\n.InputWrapper_leftLabel .InputWrapper_input input:hover , .InputWrapper_leftLabel .InputWrapper_input input:focus{\n    color:white !important;\n}\n.InputWrapper_leftLabel fieldset {\n    border:none;\n}\n.InputWrapper_inputColorWrapper{\n    width:calc(100% - 34px - #{$label-width}) !important;\n}\n.InputWrapper_leftLabel .InputWrapper_inputColor{\n    width:28px;\n    height:28px;\n    margin-left:6px;\n    border:none;\n    border-radius: 4px;\n}\n.InputWrapper_SelectKind {\n    z-index: 2;\n    position: absolute;\n    right:0px;\n}\n.InputWrapper_SelectKind_zIndex {\n    z-index: 3;\n}\n.InputWrapper_SelectKind .SelectWrapper1_theValue{\n    font-size:10px;\n    color:$input-color-bold;\n}\n.InputWrapper_input input::-webkit-outer-spin-button,\n.InputWrapper_input input::-webkit-inner-spin-button {\n  -webkit-appearance: none !important;\n  margin: 0!important;\n}\n\n.InputWrapper_bottomLabel{\n    width: 100%;\n    flex-direction: column-reverse;\n}\n.InputWrapper_bottomLabel .InputWrapper_input {\n    width: 100%;\n}\n.InputWrapper_label {\n    width: $label-width;\n    height: 28px;\n    font-size:12px;\n    text-overflow: ellipsis;\n    overflow: hidden; \n    color:$color;\n}\n.InputWrapper_bottomLabel .InputWrapper_label{\n    /* margin-top:6px; */\n    text-align: center;\n}\n.InputWrapper_bottomLabel .InputWrapper_label {\n    width:100%;\n    font-size:10px;\n    color:rgb(178,178,198);\n}\n.InputWrapper_label_text{\n    transform: translateY(-50%);\n    position: relative;\n    top:50%;\n}"],"sourceRoot":""}]);
