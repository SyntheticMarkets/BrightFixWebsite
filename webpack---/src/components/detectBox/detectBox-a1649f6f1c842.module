33, 33, 50);\n$bg-hard: rgb(16, 16, 32);\n$bg-blur: rgba(0, 0, 0, 0.4);\n\n$bg-shadow: 0 10px 15px rgba(0, 0, 0, 0.3), 0 4px 6px rgba(0, 0, 0, 0.2);\n$button-bg: rgb(106, 107, 136);\n\n$black-border: 1px rgb(48, 48, 66) solid;\n$color: rgb(255, 255, 255);\n$color-soft: rgb(190, 190, 190);\n\n$input-bg: rgb(45, 45, 65);\n$input-bg-hover: rgb(65, 65, 95);\n$input-color: rgb(162, 163, 190);\n$input-color-bold: rgb(162, 163, 200);\n$input-placeholder: rgb(82, 83, 100);\n\n$label-width: 90px;\n$modal-shadow: 0px 0px 4px rgb(20, 20, 20);\n\n$blur: blur(15px);\n// Import the variables from the JavaScript file\n\n","@import '../colors.scss';\n.detectBox {\n    width:336px;\n    height:500px;\n    line-height: normal;\n    border-radius: 8px;\n    transition:0.2s height;\n    background:rgba(16,16,32,0.93);\n    backdrop-filter: blur(5px);\n    font-family: $font;\n    box-shadow: 0px 0px 10px rgb(142,138,138);\n    // direction:ltr;\n    text-align: left;\n    animation-name: bounceInRight;\n    animation-duration: 0.2s;\n    position: fixed;\n    top:0px;\n    left:0px;\n    /* top:0px;\n    right:0px; */\n    z-index: 10000000000000000;\n}\n.detectBox_move{\n    width:28px;\n    height:28px;\n    border-radius: 50%;\n    border:1px $bg-hard solid;\n    color: var($color);\n    background:$bg-hard;\n    cursor: pointer;\n    transition:0.2s all;\n    position:absolute;\n    top:50%;\n    left:-14px;\n    z-index: 2;\n}\n.detectBox_moveLeft{\n    left:initial;\n    right:-14px;\n}\n.detectBox_move:hover{\n    color:$main;\n    border:1px $main solid;\n}\n.detectBox_move_icon{\n    width: 8px;\n    color:$input-color;\n    position: relative;\n    left: 50%;\n    margin-left: -8px;\n    top: 6px;\n    z-index: 2;\n}\n.detectBox_move:hover .detectBox_move_border{\n    border:rgba(172,168,168,0) 1px solid;\n    background:rgba(255,255,255,0)\n}\n.detectBox_move_borderHidden{\n    width:50%;\n    height:100%;\n    overflow: hidden;\n    position: absolute;\n    top:0px;\n    left:0px;\n}\n.detectBox_move_borderHiddenLeft{\n    left:50%;\n}\n.detectBox_move_borderHiddenLeft .detectBox_move_border{\n    width:calc(200% - 2px);\n    position: relative;\n    left:-100%\n}\n.detectBox_move_border{\n    width:200%;\n    height:calc(100% - 2px);\n    border-radius: 50%;\n    border:rgba(172,168,168,.3) 1px solid;\n    transition:0.2s all;\n}\n.detectBoxLeft{\n    left:0px;\n    right:initial;\n} \n/* svg{\n    width:16px;\n} */\n.detectBox * {\n    box-sizing: content-box;\n}\n\n.detectBox label{\n    margin:0px;\n}\n.detectBox_emptyText{\n    font-family: $font;\n    padding:$padding;\n    font-size:14px;\n    color:$color;\n    text-align: center !important;\n    margin-top: -15px;\n    position: relative;\n    top: 50%;\n    transform: translateY(-50%);\n}\n.detectBox_scroll{\n    /* min-height: 245px; */\n    height: calc(100% - 80px);\n    overflow: scroll;\n}\n.detectBox_scrollForHistory{\n    min-height: 291px ;\n}\n.detectBox_scrollForSignUp{\n    height: 100vh;\n}\n.detectBox_Minimaze{\n    height: 30px;\n}\n.detectBox :global .detectBox_dragArea {\n    width:100%;\n    height:30px;\n    cursor:move;\n}\n\n@-webkit-keyframes bounceInRight {\n    from {\n      margin-left:300px;\n    }\n  \n    to {\n        margin-left:0px;\n    }\n  }"],"sourceRoot":""}]);
