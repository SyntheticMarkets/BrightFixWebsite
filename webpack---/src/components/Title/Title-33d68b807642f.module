t;\n    line-height: 12px;\n}\n.Title_button svg{\n    opacity: 0;\n}\n.Title_button:hover svg{\n    opacity: 1;\n}\n.Title_button_minimaze {\n    background:#FEC242;\n}\n.Title_button_close {\n    font-size:18px;\n    position: absolute;\n    top:0px;\n    right:5px;\n}\n.Title_button_minimaze_icon{\n    width:8px;\n    color:rgb(40,40,40);\n    font-size:8px;\n    position: relative;\n    top:-3px;\n}\n.Title_button_close_icon {\n    color: $input-color;\n    width:10px;\n    cursor: pointer;\n    position: relative;\n    top:9px;\n    right:12px;\n}\n.Title_button_close_icon:hover{\n    color: $main;\n}\n.Title_button_minimaze_iconPlus{\n    font-size:10px;\n    width:6px;\n}\n.Title_switchButton{\n    height:26px;\n    width:calc(100% - 30px);\n    line-height: 26px;\n    box-sizing: border-box;\n    background:$input-bg;\n    border-radius: 6px;\n    cursor: pointer;\n    margin:5px 0px 15px 15px;\n    transition:0.2s all;\n    padding:2px;\n    position: relative;\n}\n.Title_switchButton:hover{\n    /* background:#dddee1; */\n}\n.Title_selectedButton{\n    width:50%;\n    height:100%;\n    background:$bg-hard;\n    border-radius: 6px;\n    transition:0.2s all;\n    cursor:pointer;\n    position: relative;\n}\n.Title_switchText{\n    width:50%;\n    z-index: 1;\n    text-align: center !important;\n    font-family: $font;\n    font-size:12px;\n    font-weight: 500;\n    color:$color;\n    position: absolute;\n    top:0px;\n}\n.Title_switchTextLast{\n    left:50%;\n}"],"sourceRoot":""}]);
